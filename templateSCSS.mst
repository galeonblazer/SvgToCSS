%svg-common-style {
	background-repeat: no-repeat;
}

{{#files}}
${{name}}-width: {{width}};
${{name}}-height: {{height}};

.svg-{{name}} {
	background-image: url('data:image/svg+xml;charset=utf-8{{#base64}};base64{{/base64}},{{encoded}}');
	width: ${{name}}-width;
	height: ${{name}}-height;
	@extend %svg-common-style;
}
{{/files}}